{
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "extends": ["eslint:recommended", "plugin:vue/essential", "plugin:prettier/recommended"],
  "parserOptions": {
    "sourceType": "module"
  },
  "plugins": ["vue"],
  "rules": {
    "vue/no-reserved-keys": 0,
    "declaration-block-trailing-semicolon": 0
  },
  "globals": {
    "Vue": true
  }
}
